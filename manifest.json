{
  "manifest_version": 3,
  "name": "Drive Account Chooser",
  "version": "1.2",
  "description": "Ensure the Google Account Chooser is prompted when visiting drive.google.com.",
  
  "permissions": [
    "webNavigation"
  ],

  "background": {
    "service_worker": "service_worker.js"
  },

  "icons": {
    "16": "icons/drive-16.png",
    "48": "icons/drive-48.png",
    "128": "icons/drive-128.png"
  }
}

