{
  "manifest_version": 3,
  "name": "Google Account Chooser",
  "version": "1.3",
  "description": "Choose what Google account to use when navigating to various Google sites",
  
  "permissions": [
    "webNavigation"
  ],

  "background": {
    "service_worker": "service_worker.js"
  },

  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}

